{
	"name": "@bracketed/vite-plugin-router",
	"version": "1.2.1",
	"description": "File system router for React applications using Vite.",
	"keywords": [
		"vite",
		"plugin",
		"router"
	],
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/bracketed/vite-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bracketed/vite-router"
	},
	"license": "MIT",
	"author": "@bracketed",
	"contributors": [
		"Eden Kneale <https://github.com/ninjaninja140>",
		"Felipe Bergamaschi <midisoft.corporation@gmail.com>",
		"Arthur Fiorette <me@arthur.place>"
	],
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "tsc --noEmit"
	},
	"dependencies": {
		"@types/react": "^19.0.1",
		"chalk": "^5.3.0",
		"chokidar": "^4.0.1",
		"colorette": "^2.0.20",
		"debounce": "^2.2.0",
		"prettier": "^3.4.2",
		"tslib": "^2.8.1"
	},
	"devDependencies": {
		"@types/debounce": "^1.2.4",
		"@types/node": "^22.10.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.0.2",
		"rollup": "^4.28.1",
		"typescript": "^5.7.2",
		"vite": "^6.0.3"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.0.2",
		"vite": "^6.0.3"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"lib/**/*"
	],
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"require": "./lib/index.js",
			"import": "./lib/index.js"
		},
		"./hooks": {
			"types": "./lib/hooks/index.d.ts",
			"require": "./lib/hooks/index.js",
			"import": "./lib/hooks/index.js"
		},
		"./types": {
			"types": "./lib/types/Exports.d.ts",
			"require": "./lib/types/Exports.js",
			"import": "./lib/types/Exports.js"
		}
	},
	"packageManager": "yarn@4.5.3"
}
