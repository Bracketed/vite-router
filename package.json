{
	"name": "@bracketed/vite-plugin-router",
	"version": "2.0.12",
	"description": "File system router for React applications using Vite.",
	"keywords": [
		"vite",
		"plugin",
		"router"
	],
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/Bracketed/vite-router/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Bracketed/vite-router"
	},
	"license": "MIT",
	"author": "@bracketed",
	"contributors": [
		"Eden Kneale <https://github.com/ninjaninja140>",
		"Felipe Bergamaschi <midisoft.corporation@gmail.com>",
		"Arthur Fiorette <me@arthur.place>"
	],
	"main": "lib/cjs/index.cjs",
	"module": "lib/esm/index.mjs",
	"types": "lib/cjs/index.d.ts",
	"scripts": {
		"lint": "yarn prettier --write .",
		"validate": "yarn prettier --check .",
		"build": "tsup",
		"update": "yarn upgrade-interactive",
		"test": "tsc --noEmit"
	},
	"dependencies": {
		"chokidar": "^4.0.3",
		"colorette": "^2.0.20",
		"cosmiconfig": "^9.0.0",
		"esbuild": "^0.25.9",
		"glob": "^11.0.3",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@bracketed/prettier-configuration": "^1.0.1",
		"@bracketed/tsup-configuration": "^1.0.5",
		"@bracketed/typescript-config": "^4.7.1",
		"@swc/core": "^1.13.4",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.10",
		"esbuild-fix-imports-plugin": "^1.0.21",
		"esbuild-plugin-copy": "^2.1.1",
		"esbuild-plugin-file-path-extensions": "^2.1.4",
		"esbuild-plugin-version-injector": "^1.2.1",
		"prettier": "^3.6.2",
		"react": "^19.1.1",
		"react-router-dom": "^7.8.1",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.3"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-router-dom": "^7.0.2",
		"vite": "^7.1.3"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"lib"
	],
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/index.d.mts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/index.d.ts",
				"default": "./lib/cjs/index.cjs"
			}
		},
		"./components": {
			"import": {
				"types": "./lib/esm/components/index.d.mts",
				"default": "./lib/components/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/components/index.d.ts",
				"default": "./lib/cjs/components/index.cjs"
			}
		},
		"./types": {
			"import": {
				"types": "./lib/esm/types/index.d.d.mts"
			},
			"require": {
				"types": "./lib/cjs/types/index.d.d.ts"
			}
		},
		"./virtual": {
			"import": {
				"types": "./lib/esm/virtual.d.d.mts"
			},
			"require": {
				"types": "./lib/cjs/virtual.d.d.ts"
			}
		}
	},
	"prettier": "@bracketed/prettier-configuration",
	"packageManager": "yarn@4.9.3"
}
